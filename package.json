{
  "name": "lambda-multicast",
  "version": "1.0.0",
  "description": "Proxy a single request to multiple destinations using AWS Lambda",
  "main": "index.js",
  "engines": {
    "npm": "~3.10",
    "node": "~6.10"
  },
  "scripts": {
    "prettify": "prettier --write '**/*.ts'",
    "lint": "check-node-version --package && tsc --noEmit --project . && prettier --list-different '**/*.ts' && echo lint OK",
    "lint-watch": "chokidar '**/*.ts' --ignore node_modules --debounce 100 --initial --command 'npm run --silent lint'",
    "test": "check-node-version --package && mocha --require ts-node/register $(npm run --silent test-list)",
    "test-watch": "mocha --require ts-node/register --watch --watch-extensions ts $(npm run --silent test-list)",
    "test-list": "find . -name '*.spec.ts' -not -path './node_modules/*'",
    "precommit": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jareware/lambda-multicast.git"
  },
  "author": "Jarno Rantanen <jarno@jrw.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jareware/lambda-multicast/issues"
  },
  "homepage": "https://github.com/jareware/lambda-multicast#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.34",
    "axios": "^0.17.1",
    "browserify": "^15.1.0",
    "chai": "^4.1.2",
    "check-node-version": "^3.1.1",
    "chokidar-cli": "^1.2.0",
    "husky": "^0.14.3",
    "mocha": "^4.0.1",
    "prettier": "^1.10.2",
    "ts-node": "^3.3.0",
    "tsify": "^3.0.4",
    "typescript": "^2.6.2"
  }
}
